<?xml version="1.0"?>
<sheets>

	<sheet name="Employee Sheet" class="com.stanrnd.excel.xml.Employee">
		<title text="Employee Details" foreground="GREEN" />
		<columns>
			<column field-name="name" />
			<column field-name="address" width="200" />
			<column field-name="department" order="3">
				<header text="Department" />
			</column>
			<column field-name="department">
				<header text="Department" underline="true" />
				<data foreground="RED" background="YELLOW" bold="true" />
			</column>
		</columns>
	</sheet>
	
	<sheet name="Project Sheet" class="com.stanrnd.excel.xml.Project">
		<title text="Project Details" />
		<columns>
			<column field-name="name" />
			<column field-name="technology" />
			<column field-name="clients" />
		</columns>
	</sheet>
	
</sheets>
